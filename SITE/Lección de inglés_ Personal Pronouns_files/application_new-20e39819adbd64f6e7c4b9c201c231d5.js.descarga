	rep_abuse='<div class="glyphicon glyphicon-remove report-link" style="color:red; margin-top:3px;margin-right:3px; position:relative;" data-published="'+activity.published+'" data-user="'+activity.user+'" data-body="'+activity.body+'" data-channel="'+activity.channel_name+'" data-name="'+activity.actor.displayName+'" "></div>'
}

	var time = $(
	'<div class="activity-time">' + 
		'<a class="timestamp">' + 
			'<span class="time" title="' + activity.published + '" data-activity-published="' + activity.published + '">' + PusherChatWidget.timeToDescription(activity.published) + '</span>' + 
    		rep_abuse +
    	'</a>' +    
	'</div>');
	message.append(time);
// El span 'report-link' tenia tooltip: title="Reportar mal uso"
	content.append(message);
  
	return content;
};


