                if(cont<1){
                    var o = $(this);

                    o.addClass('rspopen');
                    $('.dropdown-menu.menu-header',o).addClass('open');
                    cont = 1;
                }
            });
            $('.dropdown').click(function(){
                if(cont>0){
                    var o = $(this);

                    o.toggleClass('rspopen');
                    if(o.hasClass('rspopen')){
                        $('.dropdown-menu.menu-header',o).addClass('open');
                    }else{
                        $('.dropdown',o).removeClass('rspopen');
                        $('.dropdown-menu.menu-header',o).removeClass('open');
                    }
                }
            });
        }
        else{
            $('.dropdown').click(function(){
                var o = $(this);

                o.toggleClass('rspopen');
                if(o.hasClass('rspopen')){
                    $('.dropdown-menu.menu-header',o).addClass('open');
                }else{
                    $('.dropdown',o).removeClass('rspopen');
                    $('.dropdown-menu.menu-header',o).removeClass('open');
                }
            });
        }
