function resetMenu(){
  $(".cMenuArrow").stop(true,true).hide();
  $('.dropdown-menu').stop(true,true).hide();
  $('.meter > span').stop(true,true).css({width: 30});
  $('.dropdown').stop(true,true).removeAttr("showFlag");
  $('.dropdown > div > span').each(function(e){$(this).css("color","")});
}

function showMenu(e,div){
  var animationTime = 500;
  e.stopPropagation();
  if(!$(div).attr("showFlag")){
    resetMenu();
    var color = $(div).attr("color");
    $(div).find('.meter > span').stop(true, false).animate({
      width: "100%", 
      color: color
    }, { duration: animationTime, queue: false });
    $(div).find(".cMenuArrow").stop(true, true).animate({
        height:"show",
        opacity:"show"
    },{ duration: animationTime, queue: false });
    $(div).find('.dropdown-menu').stop(true, true)
      .animate({
        height:"show",
        opacity:"show"
    },{ duration: animationTime, queue: false });
  }  
}



