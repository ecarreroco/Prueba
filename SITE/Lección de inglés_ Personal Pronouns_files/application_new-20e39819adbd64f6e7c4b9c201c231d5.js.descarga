PusherChatWidget.prototype._login = function(email,nickname, autologin, pic, style){
	var self = this;
  
 	console.log("***************");
 	console.log("***************");
 	console.log("**** login ****");
 	console.log("***************");
 	console.log("***************");
 	console.log("**** origen: "+self.settings.origen); 
	console.log("**** email: "+email);
	  
	self._messageInputEl.val('');
	self._emailEl.val(email); 

	$.cookie('chat-email', email, {path: '/'});
	$.cookie('chat-nickname', nickname, {path: '/'});

	self.settings.email = email;
	self.settings.nickName = nickname;
	self.settings.image = pic;
	self.settings.style = style;

	logged_user_email = email
	logged_user_name = nickname
	logged_user_image = pic
	logged_user_style = style

	var imageInfo = pic;
	_image = pic;
	_image_avatar = pic;

	// Me suscribo al canal de presence una vez que me logueo, solo si es el canal publico
	this._presenceChannel = this._pusher.subscribe(this.settings.presenceChannelName);

	// Si el origen es 'widget' no se necesita conocer los usuarios activos
	// Por tanto, no hacen falta los binds del canal de presencia
	if (self.settings.origen=="widget") 
		return;

	if (!this.settings.channelName.startsWith("private")){
		
		this._presenceChannel.bind('pusher:subscription_succeeded', function(members) {
			console.log("*> presenceChannel:subscription_succeeded -> refreshActiveUsers() <*")
		
			var me = self._presenceChannel.members.me;
			var userId = me.id;
			var userInfo = me.info;
		
			members.each(function(member) {
				addMember(member.id, member.info);
	 	 	});

	 	 	refreshActiveUsers();
		});
		
		this._presenceChannel.bind('pusher:member_added', function(member) {
      		console.log("*> presenceChannel:pusher:member_added -> refreshActiveUsers() <*")

      		addMember(member.id, member.info);
			refreshActiveUsers();
		});
		
		this._presenceChannel.bind('pusher:member_removed', function(member) {
			console.log("*> presenceChannel:pusher:member_removed -> refreshActiveUsers() <*")

			removeMember(member.id, member.info);
			refreshActiveUsers();
		});
		
		this._presenceChannel.bind('member_added', function(member) {
			console.log("*> presenceChannel:member_added -> refreshActiveUsers() <*")

			addMember(member.user_id, member.user_info);
			refreshActiveUsers();
		});
		
		this._presenceChannel.bind('member_removed', function(member) {
	 	 	console.log("*> presenceChannel:member_removed -> refreshActiveUsers() <*")

		  	console.log(member);
			removeMember(member.user_id, member.info);
			refreshActiveUsers();
		});

	}
}



/* @private */
PusherChatWidget.prototype._sendChatButtonClicked = function() {
	this._messageInputEl.attr('readonly');
	this._buttonSend.prop('disabled',true);
	var self = this;
	var nickname = $.trim(self.settings.nickName);
	
	var email = $.trim(self.settings.email);
	var message = $.trim(this._messageInputEl.val());
	if (!message) {
		alert('por favor, ingrese su mensaje');
		this._messageInputEl.removeAttr('readonly');
		this._buttonSend.prop('disabled',false);
		return;
	}

	var tab = $('.nav-tabs > li.active');

	var chatInfo = {
		nickname : nickname,
		email : email,
		text : message,
