function posiciona()
{

  var dmY=$(".cDivHeadMenuRow").offset().top;
  var dmH=$(".cDivHeadMenuRow").height();

  if ( $(".notif-cnt").is(":visible") )
  {
    var headBott=dmY+dmH;

    var dmY2=$(".notif-cnt").offset().top;
    var dmH2=$(".notif-cnt").height();

    var notifBott=dmY2+dmH2;

    //console.log("headBott",headBott);
    //console.log("notifBott",notifBott);


    if (notifBott>headBott+20)
      {
      //console.log("A LA VISTA");
    
      dmY=dmY2;
      dmH=dmH2;
      var off=0;
      
      }
    else
      {
      //console.log("NO A LA VISTA");
      var off=20;
      }

  }

  //console.log("* dmY *",dmY);
  //console.log("* dmH *",dmH);

  var y1=dmY+dmH+off;

  var dfY=$(".cDivMenuFoot").offset().top;

  //console.log("* dfY *",dfY);

  var y2=dfY-5;

  var pv=y2-y1;

  var altura=$(".rightside").height();

  //console.log("* y1 *",y1);
  //console.log("* y2 *",y2);
  //console.log("* pv *",pv);
  //console.log("* altura *",altura);

  if (pv>=altura) // Cabe entero
  {
    //console.log("* cabe *");
    var posx=$(".rightside").offset().left;
    $(".rightside").offset({ top: y1, left: posx });    
  }
  else //Hay que cortar
  {
    //console.log("* cortar *");
    $(".rightside").offset({ top: y2-altura, left: posx }); 
  }


}




