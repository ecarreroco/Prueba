$(document).ready( function(){
  function startSpinner(){
    console.log( "Start" );
    $("#spinner_container_bg").show();
    var target = document.getElementById('spinner_container');
    window.spinner.spin(target);
  }

    function stopSpinner(){
      $("#spinner_container_bg").hide();
      window.spinner.stop();
    }

/*
    $(document).ajaxStart(function() {
        startSpinner();
    }).ajaxStop(function() {
      stopSpinner();
    });
*/

  var opts = {
      lines: 13, // The number of lines to draw
      length: 7, // The length of each line
      width: 3, // The line thickness
      radius: 6, // The radius of the inner circle
      corners: 1, // Corner roundness (0..1)
      rotate: 0, // The rotation offset
      color: '#fff', // #rgb or #rrggbb
      speed: 2, // Rounds per second
      trail: 60, // Afterglow percentage
      shadow: false, // Whether to render a shadow
      hwaccel: false, // Whether to use hardware acceleration
      className: 'spinner', // The CSS class to assign to the spinner
      zIndex: 2e9, // The z-index (defaults to 2000000000)
      top: '0', // Top position relative to parent in px
      left: '0' // Left position relative to parent in px
    };
  

  window.spinner = new Spinner(opts);
  $('body').append('<div id="spinner_container"></div><div id="spinner_container_bg"></div>');




});

// Global variables
    
//var host = "http://bing-translate-tts-demo.craic.com";
var host = "/txt2spch";
var audio_div;


var play_click=function(e)
{
  console.log("* play_click *");

  console.log($(e.target).attr("class"));
  if ($(e.target).hasClass("ss-play-green"))
     var color="-green";
  else if ($(e.target).hasClass("ss-play-yellow"))
     var color="-yellow";
  else if ($(e.target).hasClass("ss-play-red"))
     var color="-red";
  else
     var color="";
  console.log("*{ play_click ("+color+") }*");
  e.preventDefault();
  if (typeof contador ==='undefined')
  {
    progreso_id=play_start(color);
    var data = $(this).attr("href").split("#")[1];
    audio_div = this;

    play_load(data,color);
  
  }
  else
    console.log("*{ 'Play' en curso. }*");                  
} 
        
        
var play_start=function(color)
{
  console.log("*{ play_start("+color+") }*");
  contador=0;
  return window.setInterval(function(){
    contador++;
    console.log(contador);
    if (contador % 2 ==0)
    {
      audio_div.parentNode.className="playmp3-radial"+color+" progress"+color+"-0";
      $(audio_div).css('background-image','url(/assets/nothing.png)');
    }
    else
    {
      audio_div.parentNode.className="playmp3-radial"+color+" progress"+color+"-100";
      $(audio_div).css('background-image','');            
    }
    }, 500);
}

        
var play_load = function(t,color)
{
  console.log("*{ play_load("+color+") }*");

  /* AQUI FUNCIONA */

  text=t.trim();
  if (text.slice(-1)==".")
  {
    text=text.substring(0,text.length-1);
  }
  text = text.replace(/\//g, ","); // '/' -> ','
  text = text.split(".").join("");

  console.log("["+text+"]");

  var file="";
  jQuery.ajax({
    url:host,
    data: {text: text},
    success: function(result) {
      console.log("* SUCCESS *");
      file=result.response;
    },
    async:   false
  });  
  console.log(file);

  //file=" http://s3.amazonaws.com/sk.CursoIngles/mp3s/113/original/0e7f6370808497ea1a233c77ab63cb4e3e8340c320130315-3255-1arl6p0.mp3";
  var audio = new Audio();
  audio.src = file
  audio.addEventListener('ended',function()
  {
    console.log("*{ audio end }*");    
    window.clearInterval(progreso_id);
    audio_div.parentNode.className="playmp3-radial"+color+" progress"+color+"-100";
    $(audio_div).css('background-image','url(/assets/play_icon'+color+'.png)');
    delete contador;
  });
  audio.addEventListener('error', function failed(e) {
    console.log("*{ audio error }*");    
    window.clearInterval(progreso_id);
    audio_div.parentNode.className="playmp3-radial"+color+" progress"+color+"-100";
    $(audio_div).css('background-image','url(/assets/play_icon'+color+'.png)');
    delete contador;
  }, true);

  audio.play();

}


var play_update=function(e,color){
  var p=e.target;
  // update the radial progress bar.
  var progress = Math.floor((100 / p.duration) * p.currentTime/5)*5;
  if (audio_div!=null){
    audio_div.parentNode.className="playmp3-radial"+color+" progress"+color+"-"+progress;
  }
}
    



$(document).ready(function() 
{
    console.log("*{ text_to_speech.js }*");

    $(".playmp3").each(function(){
      $(this).attr("class","ss-play playmp3");
    });


    $('[class^="ss-play"]').click(play_click);

    //$(".ss-play").click(play_click);
    //$(".ss-play-green").click(play_click);
    //$(".ss-play-yellow").click(play_click);

});


