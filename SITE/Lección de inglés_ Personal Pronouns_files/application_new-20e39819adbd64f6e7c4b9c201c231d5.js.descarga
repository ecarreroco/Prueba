		//user1_image: _image,
		//user2: tab.data('user2Email')
	};
	console.log("*** self.settings.style ***")
	console.log(self.settings.style)
	console.log("*** _sendChatButtonClicked: chatInfo ***")
	console.log(chatInfo);
	self._sendChatMessage(chatInfo);
};

/* @private */
PusherChatWidget.prototype._sendChatMessage = function(data) {
	var self = this;
 
 	self._messageInputEl.attr('readonly');
 	self._buttonSend.prop('disabled',true);
	$.ajax({
		url : this.settings.chatEndPoint,
		type : 'post',
		data : {
			'chat_info' : data,
			'channel_name' : this.settings.channelName
		},
		complete : function(xhr, status) {
			Pusher.log('Chat message sent. Result: ' + status + ' : ' + xhr.responseText);
			if (xhr.status === 200) {
				self._messageInputEl.val('');
			}
			self._messageInputEl.removeAttr('readonly');
			self._buttonSend.prop('disabled',false);
		},
		success : function(result) {
			
		}
	})
};

/* @private */
PusherChatWidget.prototype._startTimeMonitor = function() {
	var self = this;

	setInterval(function() {
		var times = $('a.timestamp > span');

		times.each(function(i, el){
		  var time = $(el).data('activity-published');
		  var newDesc = PusherChatWidget.timeToDescription(time);
		  $(el).text(newDesc);
		});
	}, 10 * 1000)
};





